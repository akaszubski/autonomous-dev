{
  "name": "autonomous-dev",
  "version": "3.22.0",
  "description": "Autonomous development plugin for Claude Code - 20 AI agents, 22 skills, batch processing, MCP auto-approval, automation hooks",
  "author": "akaszubski",
  "repository": "https://github.com/akaszubski/autonomous-dev",
  "agents": 20,
  "skills": 22,
  "commands": [
    "/auto-implement",
    "/batch-implement",
    "/align-project",
    "/align-claude",
    "/align-project-retrofit",
    "/setup",
    "/sync",
    "/status",
    "/health-check",
    "/pipeline-status",
    "/update-plugin",
    "/create-issue",
    "/research",
    "/plan",
    "/test-feature",
    "/implement",
    "/review",
    "/security-scan",
    "/update-docs",
    "/test"
  ],
  "hooks": {
    "core": 11,
    "optional": 20,
    "lifecycle": 2
  },
  "dependencies": {
    "python": ">=3.9",
    "pytest": ">=7.0.0",
    "pytest-cov": ">=4.0.0",
    "pytest-xdist": ">=3.0.0",
    "syrupy": ">=4.0.0",
    "pytest-testmon": ">=2.0.0",
    "PyYAML": ">=6.0.0"
  },
  "testing": {
    "framework": "pytest",
    "tiers": [
      "smoke",
      "regression",
      "extended",
      "progression"
    ],
    "coverage_target": "80%",
    "features": [
      "parallel execution",
      "snapshot testing",
      "smart test selection"
    ]
  }
}
