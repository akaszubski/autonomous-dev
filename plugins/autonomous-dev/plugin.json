{
  "name": "autonomous-dev",
  "version": "3.50.0",
  "description": "Autonomous development plugin for Claude Code - 16 AI agents, 40 skills, batch processing, MCP auto-approval, automation hooks",
  "author": "akaszubski",
  "repository": "https://github.com/akaszubski/autonomous-dev",
  "agents": 16,
  "skills": 40,
  "commands": [
    "/implement",
    "/align",
    "/setup",
    "/sync",
    "/health-check",
    "/create-issue",
    "/worktree"
  ],
  "hooks": {
    "core": 11,
    "optional": 20,
    "lifecycle": 2
  },
  "dependencies": {
    "python": ">=3.9",
    "pytest": ">=7.0.0",
    "pytest-cov": ">=4.0.0",
    "pytest-xdist": ">=3.0.0",
    "syrupy": ">=4.0.0",
    "pytest-testmon": ">=2.0.0",
    "PyYAML": ">=6.0.0"
  },
  "testing": {
    "framework": "pytest",
    "tiers": [
      "smoke",
      "regression",
      "extended",
      "progression"
    ],
    "coverage_target": "80%",
    "features": [
      "parallel execution",
      "snapshot testing",
      "smart test selection"
    ]
  }
}
